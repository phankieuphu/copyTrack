{
  "name": "copy-track",
  "displayName": "copyTrack",
  "author": {
    "name": "phu.phan",
    "email": "phankieuphu@gmail.com"
  },
  "license": "MIT",
  "description": "Copied list for Visual Studio Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/phankieuphu/copyTrack"
  },
  "bugs": {
    "url": "https://github.com/phankieuphu/copyTrack/issues"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "copyTracker",
          "title": "Copy Tracker",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "copyTracker": [
        {
          "type": "webview",
          "id": "copiedItemsView",
          "name": "Copied Items"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
